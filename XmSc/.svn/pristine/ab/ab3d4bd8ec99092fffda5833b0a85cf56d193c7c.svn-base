html{
	height: 100%;
}
body{
	font-family: "Microsoft YaHei" ! important;
	background: #efeff5;
	height: 100%;
	color: #000000;
	overflow-x:hidden; 
}
*{
	margin: 0;
	padding: 0;
}
ul,ol {list-style:none;}
a{text-decoration: none;color: #666;}
img{border: 0; width: 100%}

input:-moz-placeholder,
textarea:-moz-placeholder {
    color: #999999;
}

input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
    color: #999999;
}

input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
    color: #999999;
}
.clearfix:after{ content: "."; display: block; height: 0; clear: both; visibility: hidden; font-size:0; }
.clearfix{ zoom:1; }
.mask{
	position: fixed;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.5);
}
.none{
	display: none;
}
/** 中奖纪录 start **/
.record_c{
	width: 100%;
	height: 100%;
	background: #fff;
	position: fixed;
	left: 0px;
	top: 0px;
}
.top_title{
	background: url("../images/top.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/top.png*/ 0 0 no-repeat;
	background-size: 100%;
	width: 16rem;
	height: 2.5rem;
	margin: 0 auto;
	font-size: 1rem;
	color: #fff;
	text-align: center;
	line-height: 2.5rem;
}
.datalist{
	width: 96%;
	margin: 0 auto;
}
.datalist .row{
	border-bottom: #afafaf 1px solid;
	padding: .5rem  0;
	font-size: .7rem;
}
.datalist .row.title{
	border-bottom: #d02d48 1px solid;
	font-size: .8rem;
	font-weight: bold;
}
.datalist .row .col{
	float: left;
	width: 30%;
}
.datalist .row .col.center{
	text-align: center;	
    word-wrap: break-word;
}
.datalist .row .col.left{
	text-align: left;
}
/** 中奖纪录 end **/
.detail{
	font-size: .8rem;
  	padding: 10px;
}


.boxcontent {
	margin: 15px 15px 0;
	border-radius: 5px;
	padding: 2px;
	background-color: #f9eeaf;
	-webkit-box-shadow: 0 0 6px rgba(0, 0, 0, 0.5);
	-moz-box-shadow: 0 0 6px rgba(0, 0, 0, 0.5);
	-o-box-shadow: 0 0 6px rgba(0, 0, 0, 0.5);
	box-shadow: 0 0 6px rgba(0, 0, 0, 0.5);
}
.box {
    border-radius: 5px;
    font-size: 14px;
    border: 1px dashed #e25f3c;
}

.box .title-red {      
	background: url("../images/p4.jpg")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/p4.jpg*/ no-repeat 0 0;
	background-size: 100%;
	width: 3.875rem;
	height: 1rem;
	border-radius: 3px 3px 3px 0;
	color: #ffffff;
	height: 22px;
	margin: -1px; 
	text-indent: 5px;    
}

.box .detail2 {
	padding: 15px;
}
.bigwheel{
	width: 16rem;
	height: 100%;
	background-color: #d02d48;
	background: url("../images/bg.jpg")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/bg.jpg*/ left top no-repeat;
	background-size: 100%;
	margin: 0 auto;
}
.scence{
	width: 16rem;
	height: 14rem;
	position: relative;
}
.scence .wheel{
	background: url("../images/wheel.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/wheel.png*/ 0 0 no-repeat;
	background-size: 100%;
	width: 12rem;
	height: 12rem;
	position: absolute;
	left: 50%;
	margin-left: -6rem;
	top: 0px;
}
.scence .pointer{
	background: url("../images/pointer.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/pointer.png*/ 0 0 no-repeat;
	background-size: 100%;
	width: 3.5rem;
	height: 4.2rem;
	position: absolute;
	left: 50%;
	margin-left: -1.7rem;
	top: 30%;
}
.scence .belt{
	background: url("../images/p3.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/p3.png*/ 0 0 no-repeat;
	background-size: 100%;
	width: 15.7rem;
	height: 2.75rem;
	position: absolute;
	left: 50%;
	margin-left: -7.85rem;
	top: 70%;
	color: #fff;
	font-size: .6rem;
	line-height: 3.4rem;
}
.scence .belt2{
	width: 15.7rem;
	height: 2.75rem;
	position: absolute;
	left: 50%;
	margin-left: -7.85rem;
	top: 70%;
	color: #fff;
	font-size: .6rem;
	line-height: 3.4rem;
	text-shadow: #000 1px 1px 1px;
}
.scence .belt2 .left{
	float: left;
	margin-left: 1rem;
}
.scence .belt2 .right{
	float: right;
	margin-right: 1rem;
}
.scence .data{
	color: #ff5757;
	font-size: .8rem;
	position: absolute;
	bottom: 3px;
	width: 100%;
	text-align: center;
}
.scence .data span{
	color: #fff;
}

.reward_list{
  margin: 0 .2rem; 
}
.reward_list li{
	list-style: none;
	padding: .2rem 0px;
}
.reward_list li .phone{
	float: left;
	width: 50%;
	text-align: left;
}
.reward_list li .name{
	float: left;
	width: 50%;
	text-align: right;
}

.dialog_bg{
	background: url("../images/dialog_bg.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/dialog_bg.png*/ 0 0 no-repeat;
	background-size: 100%;
	width: 14.5rem;
	height: 11.825rem;
	position: fixed;
	left: 50%;
	top: 50%;
	margin-left: -7.25rem;
	margin-top: -5.9rem;
}
.dialog_bg .content{
	color: #d02d48;
	display: table;
	width: 80%;
	text-align: center;
	position: absolute;
	top: 20%;
	left: 10%;
}
.dialog_bg .content .msg{
	display: table-cell;
	height: 130px;
	vertical-align: middle;
	padding: 10px;
	font-size: .8rem;
}
.dialog_bg .btn{
	position: absolute;
	bottom: 5px;
	left: 0px;
	width: 100%;
	text-align: center;
	line-height: 2rem;
	height: 2rem;
	color: #fff;
}


.reward_bg{
	position: fixed;
	width: 14rem;
	height: 100%;
	left: 50%;
	top: 10%;	
	margin-left: -7rem;
}
.reward_bg .reward_title{
	background: url("../images/reward_title.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/reward_title.png*/ 0 0 no-repeat;
	background-size: 100%;
	height: 5.375rem;
	width: 14rem;

}
.reward_bg .reward_msg{
	background: url("../images/reward_center.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/reward_center.png*/ 0 0 repeat-y;
	background-size: 100%;	
	min-height: 8rem;
  	overflow: hidden;
}
.reward_bg .reward_btn{
	background: url("../images/reward_bottom.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/reward_bottom.png*/ 0 0 no-repeat;
	background-size: 100%;
	width: 14rem;
	height: 2.2rem;
	position: absolute;
	left: 0px;	
	text-align: center;
	line-height: 2rem;	
	color: #fff;
}
.reward_data{
	width: 80%;
	margin: 0 auto;
	font-size: .8rem;
}/*
.input{
	background: url(http://static.jiouyun.com/source/modules/bigwheel/template/style/images/input.png) 0 0 no-repeat;
	background-size: 100%;
	width: 10.6rem;
	height: 1.85rem;
	position: relative;
}*/
.input_realname{
    background: url("../images/input_realname.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/input_realname.png*/ 0 0 no-repeat;
    background-size: 100%;
    width: 10.6rem;
    height: 1.85rem;
    position: relative;
}
.input_address{
    background: url("../images/input_address.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/input_address.png*/ 0 0 no-repeat;
    background-size: 100%;
    width: 10.6rem;
    height: 1.85rem;
    position: relative;
}
.input_mobile{
    background: url("../images/input_mobile.png.jpg")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/input_mobile.png*/ 0 0 no-repeat;
    background-size: 100%;
    width: 10.6rem;
    height: 1.85rem;
    position: relative;
}
.input input{
	position: absolute;
	left: 1.4rem;
	top: .4rem;
	height: 1rem;
	line-height: 1rem;
	border: none;
	background: #ffecbf;
	outline: none;
	font-size: .7rem;
}
.reward_data p{
	margin: 5px 0px;
}
.reward_data .label{
	color: #c92323;
	font-size: .6rem;
	margin-top: 20px;
}
.reward_data .red{
	color: red;
}
.reward_data .code{
	color: #6c7ab6;
}

.close{
	background: url("../images/close.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/close.png*/ 0 0 no-repeat;
	background-size: 100%;
	width: 32px;
	height: 32px;
	position: absolute;
	right: 10px;
	top: 10px;
}
.reward_list_c{
	width: 100%;
	height: 2.6rem;
	overflow: hidden;
}
.footer{
  width: 100%;
  text-align: center;
  font-size: .8rem;
  color: #fff;
  margin: .5rem 0;
}
.share{
	background: url("../images/share.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/share.png*/ 0 0 no-repeat;
	background-size: 100%;
	width: 16rem;
	height: 11.3rem;
	margin: 0 auto;
}
.share .keys{
	  position: absolute;
	  top: 8.35rem;
	  left: 9rem;
	  color: red;
	  font-size: .8rem;  
	  text-align: center;
	  text-shadow: #fff 1px 0 0,#fff 0 1px 0,#fff -1px 0 0,#fff 0 -1px 0;
}

#wrapper_datalist{
	position: absolute;
	z-index: 1;
	top: 52px;
	bottom: 12px;
	left: 0px;
	width: 100%;
	overflow: auto;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
#wrapper_datalist .scroller{
	width: 100%;
}

.datalist .row{
	padding-left: 1rem;
	padding-right: 1rem;
}
.datalist .row .col.right{	
	text-align: right;
}
.datalist .row .col{	
	width: 50%;	
}
.record-content{
	margin:0.5rem;
	background-color: #f0f0f0;	
}
.record-content .row:last-child{
	border:none;
}
.record-content .exchanged{
	color: #d02d48;
}
.record-content .exchange{
	display: inline-block;
	height: 1.4rem;
	line-height: 1.4rem;
	background-color: #d02d48;	
	color:#fff;	
	border-radius:3px;	
	padding:0 .8rem;
	background: -webkit-linear-gradient(top, #ea435f, #c72741);
	background: linear-gradient(top, #ea435f, #c72741);
}
#exchangeMask .reward_title {
	  background: url("../images/reward_title2.png")/*tpa=http://static.jiouyun.com/source/modules/bigwheel/template/style/images/reward_title2.png*/ 0 0 no-repeat;
	  height: 3.775rem;
	  background-size: 100% 100%;
}
#exchangeMask .reward_msg{
	min-height: 3rem
}
#exchangeMask .reward_data{
	text-align: center;
}
#exchangeMask .reward_data .sn{
	background-color: #ffecbf;
	height: 1.75rem;
	width: 10rem;
	line-height: 1.75rem;
	font-size: 0.7rem;
	border-radius:5px;
	border:2px solid #d7b97b;
	padding-left: 0.3rem;
	margin: 1.2rem 0;
}
.sn::-webkit-input-placeholder{ 
	color:#c92323 !important; 
} 
.sn:-moz-placeholder{ 
	color:#c92323 !important; 
}
#exchangeMask .submit-btn{
	width: 90%;
	margin:0 auto;
	background-color: #e9425e;
	color: #fff;
	height: 2.2rem;
	line-height: 2.2rem;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
	font-size: 0.7rem;
	text-align: center;
	letter-spacing: .4rem;
	text-indent: .4rem;
	background: -webkit-linear-gradient(top, #ea445f, #c62641);
	background: linear-gradient(top, #ea445f, #c62641);
}
#exchangeMask .reward_bg{
	  top: 30%;
	  height: auto;
}

.mask {
    background: rgba(0, 0, 0, 0.5) none repeat scroll 0 0;
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 1000;
}
.share_word {
    color: #fff;
    font-size: 1.2rem;
    line-height: 1.8rem;
    margin-top: 5rem;
    text-align: center;
}
.s_w {
    background-image: url("../../../../public/images/s_w.png")/*tpa=http://static.jiouyun.com/source/modules/public/images/s_w.png*/;
    background-size: 100% 100%;
    height: 1.375rem;
    margin: 0 auto;
    width: 12.875rem;
}
.keywords {
    color: #de4b21;
    text-shadow: 1px 0 0 #fff, 0 1px 0 #fff, -1px 0 0 #fff, 0 -1px 0 #fff;
}
.sp {
    border-bottom: 1px solid #fff;
    margin-bottom: 0.7rem;
    margin-top: 1.5rem;
}
.d2 {
    background-image: url("../../../../public/images/zw.png")/*tpa=http://static.jiouyun.com/source/modules/public/images/zw.png*/;
    background-position: right center;
    background-repeat: no-repeat;
    background-size: 5.4rem 5.4rem;
    height: 5.5rem;
    left: 50%;
    margin: 2rem auto 0 -6rem;
    position: absolute;
    text-align: center;
    width: 12rem;
}
.cz {
    color: #fff;
    font-size: 0.6rem;
    margin-top: 0.2rem;
    text-align: center;
}