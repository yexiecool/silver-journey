/*单选框*/
.ckbox {
    position: relative;
    left: 9px
}

.ckbox input[type="checkbox"] {
    opacity: 0;
}

.ckbox label {
    cursor: pointer;
    font-size: 13px;
}

.ckbox label i {
    position: absolute;
    display: inline-block;
    margin-top: 5px;
    padding-left: 15px;
}

.ckbox label:before {
    display: inline-block;
    width: 20px;
    height: 20px;
    position: absolute;
