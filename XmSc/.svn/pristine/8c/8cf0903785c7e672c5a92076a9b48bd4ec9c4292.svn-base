/*alert*/
.mod-dialog-bg {
    background: #000;
    opacity: .5;
    filter: alpha(opacity=50);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 999999;
}

.mod-dialog {
    background-color: #fff;
    border-top: none;
    width: 80%;
    border-radius: 3px 3px 0px 0px;
    position: absolute;
    z-index: 1000000;
}

.mod-dialog .dialog-nav {
    background: #d63333;
    border-radius: 3px 3px 0px 0px;
    color: #fff;
    font-size: 20px;
    height: 40px;
    line-height: 40px;
}

.mod-dialog .dialog-title {
    padding-left: 20px;
}

.mod-dialog .dialog-close {
    font-family: FontAwesome;
    font-weight: normal;
    font-style: normal;
    position: absolute;
    height: 24px;
    right: 8px;
    width: 24px;
}

.mod-dialog .dialog-close:before {
    content: "\f00d";
    color: #fff;
}

.mod-dialog .dialog-main {
    padding: 20px;
    text-align: center;
    color: #aaa;
}

.mod-dialog .dialog-console {
    padding-top: 20px;
}

.mod-dialog .dialog-console a {
    display: inline-block;
    padding: 0px 5px 3px;
    border-radius: 3px;
    font-size: 16px;
    line-height: 30px;
    text-align: center;
    width: 40%;
}

.mod-dialog .dialog-console .console-btn-confirm {
    background-color: #d63333;
    color: #fff;
}

.mod-dialog .dialog-console .console-btn-cancel {
    background: none repeat scroll 0% 0% #eee;
    color: #aaa;
    margin-left: 12px;
}

