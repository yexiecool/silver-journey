/*输入提示框*/
.tooltip {
    position: absolute;
    z-index: 9999;
    display: block;
    font-size: 14px;
}

.tooltip.in {
    filter: alpha(opacity=100);
    opacity: 1;
}

.tooltip.top {
    padding: 5px 0;
    margin-top: -3px
}

.tooltip-inner {
    max-width: 360px;
    padding: 10px 10px;
    color: #fff;
    text-align: left;
    text-decoration: none;
    background-color: #d63333;
    border-radius: 4px
}

.tooltip-arrow {
    position: absolute;
    width: 0;
    height: 0;
    border-color: transparent;
    border-style: solid
}

.tooltip.top .tooltip-arrow {
    bottom: 0;
    left: 50%;
    margin-left: -5px;
    border-width: 5px 5px 0;
    border-top-color: #d63333;
}

