.photo-tag{}
.leftTag,
.rightTag {position: absolute}
.tagAnimation {width:21px; height:21px;	position:absolute; top:2px}
.animationContainer{position:absolute;top:0;left:0;text-align:center;width:21px;height:21px}
.black1,
.black2 {
    position: relative;
    width: 21px;
    height: 21px;
    background-image: url(http://imgs.xin.com/xinwap/images/page/black2.png);
    background-repeat: no-repeat;
    opacity: .6;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=60)";
    filter: alpha(opacity=60);
    display: block
}
.tagContent {
    font-size: 12px;
    position: relative;
    left: 17px;
    line-height: 26px;
    opacity: .8;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
    filter: alpha(opacity=80)
}

.tagContent:before {
    content: ' ';
    float: left;
    width: 12px;
    height: 26px;
    background:url(http://imgs.xin.com/xinwap/images/page/left_tag.png) no-repeat;
    background-size:12px 26px
}

.tagContentInfo {
    overflow: hidden;
    background: #000;
    opacity: .9;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=90)";
    filter: alpha(opacity=90);
    color: #fff;
    font-size: 12px;
    display:inline-block;
}
.tagContentInfo a {text-decoration:none; color:#fff}
.leftTag .tagContentInfo {
    -webkit-border-radius: 0 5px 5px 0;
    border-radius: 0 5px 5px 0;
    padding-right: 8px
}
.leftTag .tagAnimation {left:-18px}
.rightTag .tagContent {left:0}
.rightTag .tagContent:before {
    float: right;
    width: 12px;
    height: 26px;
    background:url(http://imgs.xin.com/xinwap/images/page/right_tag.png) no-repeat;
    background-size:12px 26px
}

.rightTag .tagContentInfo {
    -webkit-border-radius: 5px 0 0 5px;
    border-radius: 5px 0 0 5px;
    padding-left: 8px
}

.rightTag .tagAnimation{right:-17px}
.brand-bg,
.point-bg,
.user-bg {
    position: relative;
    top: 50%;
    width: 9px;
    height: 9px;
    background-image: url(http://imgs.xin.com/xinwap/images/page/user-bg.png);
    background-repeat: no-repeat;
    display: block;
    margin: -4.5px auto 0;
    vertical-align: middle;
    -webkit-animation: brand 1.95s infinite
}
@-webkit-keyframes brand {
    0% {
        -webkit-transform: scale(.8, .8)
    }

    10% {
        -webkit-transform: scale(1.25, 1.25)
    }

    100%,
    20% {
        -webkit-transform: scale(1, 1)
    }

}

.black1 {-webkit-animation: blackFirst 1.95s infinite}
@-webkit-keyframes blackFirst {
    0% {
        -webkit-transform: scale(.03, .03)
    }

    20% {
        -webkit-transform: scale(.03, .03);
        opacity: 1;
        -ms-filter: none;
        filter: none
    }

    66% {
        -webkit-transform: scale(1, 1);
        opacity: .05;
        -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=5)";
        filter: alpha(opacity=5)
    }

    100%,
    67% {
        -webkit-transform: scale(.03, .03)
    }

}

.black2 {-webkit-animation: blackSecond 1.95s infinite}
@-webkit-keyframes blackSecond {
    0% {
        -webkit-transform: scale(.03, .03)
    }

    44% {
        -webkit-transform: scale(.03, .03);
        opacity: 1;
        -ms-filter: none;
        filter: none
    }

    90% {
        -webkit-transform: scale(1, 1);
        opacity: .05;
        -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=5)";
        filter: alpha(opacity=5)
    }

    100%,
    91% {
        -webkit-transform: scale(.03, .03)
    }

}
