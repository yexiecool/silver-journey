@charset "utf-8";
/* CSS Document */
/* reset */
html,body,h1,h2,h3,h4,h5,h6,div,dl,dt,dd,ul,ol,li,p,blockquote,pre,hr,figure,table,caption,th,td,form,fieldset,legend,input,button,textarea,menu{margin:0;padding:0;}
header,footer,section,article,aside,nav,hgroup,address,figure,figcaption,menu,details{display:block; text-align: center;}
table{border-collapse:collapse;border-spacing:0;}
caption,th{text-align:left;font-weight:normal;}


html,body,fieldset,img,iframe,abbr{border:0;}
i,cite,em,var,address,dfn{font-style:normal;font-weight:normal;}
[hidefocus],summary{outline:0;}
ol,ul,li{list-style:none;}
h1,h2,h3,h4,h5,h6,small{font-size:100%;font-weight:normal;}
sup,sub{font-size:83%;}
pre,code,kbd,samp{font-family:inherit;}
q:before,q:after{content:none;content:'';}
textarea{overflow:auto;resize:none;}
label,summary{cursor:default;}
a,button{cursor:pointer;}
h1,h2,h3,h4,h5,h6,strong,b{font-weight:bold;}
del,ins,u,s,a,a:hover{text-decoration:none;}
body,textarea,input,button,select,keygen,legend{font:12px/1.14 arial,\5b8b\4f53;color:#333;outline:0;}
body{background:#fff;}
a,a:hover{color:#333;}
.clearfix:after{content:'\20';display:block;height:0;clear:both}
.clearfix{zoom:1}
.clear{ clear: both; }
/* css 重置 */
*{margin:0; padding:0; list-style:none; }
body{ background:#fff; font-family:"微软雅黑"; -webkit-tap-highlight-color:transparent;}
img{ border:0; display: block;  }
a{ text-decoration:none; color:#333;  }
.fl{ float: left;}
.fr{ float: right;}
.clear{ clear: both; }
/*index页面*/
.content{ width: 96%; margin:0 auto; height: auto; padding:2%;}
.banner{ width: 100%; color: #fff; text-align: center; padding: 1.5rem 0; font-size: 1rem; background:url('../images/ghs.jpg') top center  #00f ; background-size: 100% auto;}
.banner p{font-size: .9rem; }
.banner b{font-size: 1rem; line-height: 2.2rem;}
.banner font{font-size: .7rem;}
.list h1{ text-align: center; list-style-type: 3rem; font-size: 1.1rem; padding: .8rem 0; }
.list ul li{ width: 100%; margin: 1px 0; font-size:1rem; line-height: 1.5rem; }
.list ul li span{ float:left; margin:.2rem .5rem 0 0; display: block; width: 1rem; height: 1rem; border-radius: 1rem; color: #fff; text-align: center; line-height: 1rem; background:#4795b9;}
.list p{ font-size: 1rem; text-indent: 2em; line-height: 1.6rem;  }
.list2 p{ margin-left: 1.5rem;   text-indent: 0em;}
.list2 p.list2p{ margin-left: 0rem; line-height: 1.8rem; }

/*blue*/ 
.blue{ background: #bdeaff; margin:1rem 0;   } 
.blue tr td{ border:1px solid #fff; line-height: 1.5rem; text-indent: .5rem;  } 
.blue tr td span{font-size: .2rem; } 
/*huiyuan*/ 
.goodlist {background:url("../images/bannerlist.jpg") no-repeat bottom center #e7e7e7;
background-size: 100% auto; height: auto; padding-bottom: 6.4rem; } 
.goodlist li{ width: 15%; margin-right: 4%;   float: left; } 
