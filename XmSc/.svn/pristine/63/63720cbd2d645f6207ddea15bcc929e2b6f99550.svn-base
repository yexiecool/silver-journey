.goodcount .textcount i{ color: #d2323c; font-size: 1rem;float: right;  }

/*search*/
.topcount { position: absolute; top: 0; left: 0; z-index: 10000; width: 100%; margin-top: 10px; }
.topcount i{ float: left; margin-top: 5px; }
.topcount .search{width: 80%; float: left;height: 2.1rem; 
		line-height: 1rem;
		background: url("../images/search.png") no-repeat;
		border-radius: 0.5rem;
		color: #000;
		text-align: center;
		background-size:100% auto;
		display: block;
		border: none;}
