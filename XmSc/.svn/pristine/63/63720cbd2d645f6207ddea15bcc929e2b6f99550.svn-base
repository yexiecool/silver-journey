.goodlist div img{margin: 0 auto;  width:100%;    } 
.goodlist span{ display: block; text-align:center; color: #000001; font-size: .8rem;   margin:10px 0; } 
.banners{ width:100%; height: auto;  } 
.forecont{ background:url("../images/forebg.jpg") no-repeat top center #e7e7e7; float: left; width: 100%; background-size: 100% auto; height: auto;  } 
.forecont li.one{ margin-top: 30px;  }
.forecont li{ width: 70%; margin-left: 15%; padding: 30px 0; float: left; } 
