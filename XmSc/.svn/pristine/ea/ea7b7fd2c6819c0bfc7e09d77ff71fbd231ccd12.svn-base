*{
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}
a{
	text-decoration:none;
}
li{
	list-style: none;
}
body{
	background: #fff !important;
	font-size: 14px;
	font-family: "寰蒋闆呴粦";
}
.list{
	width: 100%;
	height:auto;
	overflow: hidden;
	padding: 0 15px;
	margin-top: 10px;
}
.list>ul>li{
	float: left;
	width: 20%;
	height:auto;
	overflow: hidden;
	text-align: center;
	color:#333333;
	margin-bottom: 10px;
}
.list>ul>li>a{
	font-size: 14px;
	color:#333;
}
.list>ul>li>a>img{
	width: 85%;
	margin-bottom: 5px;
}
.list>ul>li>a>p{ width:100%;}

/*瓒呭競蹇呬拱*/
.Supermarket,.home,.Brand,.Nomination{
	width: 100%;
	height: auto;
	overflow: hidden;
	padding: 0 15px;
	margin-top: 10px;
}

.clear{ clear: both;}

.Supermarket>div{
	float:left;
}
.Supermarket_left,.Supermarket_right{
	width: 40%;
	height:auto;
	overflow: hidden;
}
.Supermarket_right{
	width: 59%;
	margin-left: 1%;
}
.Supermarket_left>a>img{
	width: 100%;
}
.Supermarket_right_img{
	width: 100%;
	height: auto;
	overflow: hidden;
	margin-bottom: 5px;
	background: #e8f9ff;
}
.Supermarket_right_img:last-of-type{
	background: #ebedfa;
}
.Supermarket_right_img>a>div{
	float:left;
}
.Supermarket_right_img>a>div:last-of-type{
	float:right;
	width: 45%;
}
.Supermarket_right_img>a>div:last-of-type>img{
	width: 100%;
}
.Supermarket_right_img>a>div>p{
    font-size: 12px;	
    margin-bottom: 0;
}.Supermarket_right_img>a>div>span{
	display: inline-block;
	font-size:12px;
	color:#fff;
	padding: 3px 10px;
	background: #86cde1;
	border-radius:3px;
	margin-top: 5px;
}


/*鐢熸椿瀹�*/
.home>ul>li,.Brand>ul>li,.Nomination>ul>li{
	float:left;
	width: 33.33%;
	height:auto;
	overflow: hidden;
	text-align: center;
	padding: 0 10px;
}

.Nomination ul li p{
	text-align: left; 
}


.home>ul>li>a>img{
	width: 55%;
	height:69px;
}
.home>ul>li>a>p{
	width: 100%;
	height:20px;
	font-size: 12px;
	color:#fff;
	line-height: 20px;
	margin-top: 5px;
}




/*鍝佺墝绮鹃�� */
.Brand>a>img{
	width: 100%;
}
.Brand>ul>li,.Nomination>ul>li{
	padding: 0;
}
.Brand>ul>li>a>p{
	font-size: 12px;
}
.Brand>ul>li>a>img{
	width: 100%;
	height:69px;
}
.Nomination>ul{
	width: 100%;
	height:auto;
	overflow: hidden;
	padding: 5px;
}
.Nomination>ul>li>a>p{
	font-size: 12px;
}
.Nomination>ul>li>a>img{
	width: 90%;
	height:70px;
}

/*xmcs*/
.xmcs{float: left;  width: 100%;   height: auto; margin-bottom:50px;   }
.xmcs h6{ font-size:14px;color:#302d34; width: 90%; padding: 10px 5%; }
.xmcs li{width: 27%;  float: left; margin:10px 0 0 5%;  } 
.xmcs li img { width: 100%; }
.xmcs p { font-size: .8rem; margin-top: 10px; width: 100%; height: 4rem; overflow: hidden;  }
.xmcs p span{ background: #83afd5; color: #fff; padding: 3px; font-size: .8rem; border-radius: 3px;    }
.xmcs font{ color: #d2323c; float: left; font-size: 1rem;  }
.xmcs i{ color: #d2323c; font-size: 1rem;float: right;  }
