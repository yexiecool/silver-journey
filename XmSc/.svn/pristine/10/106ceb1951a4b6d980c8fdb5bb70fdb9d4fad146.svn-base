body {
	font-size: 12px;
	background-color:#F4F4F4;
	margin-top: 0px; 
	margin-left:0px;
	margin-right:0px; 
	margin-bottom:0px;
}

form{
	margin: 0px; 
	disply:inline;
	
}
#first{
	margin-top: 0px; 
	disply:inline;
	
}
*html #first{
	margin-top: 0px; 
	disply:inline;
	
}
*+html #first{
	margin-top: 0px; 
	disply:inline;
	
}
.buttonTable {
	margin: 0px; 
}



.eXtremeTable {
	margin: 0px;
	padding: 0px;
}

.eXtremeTable select {
	font-family: verdana, arial, helvetica, sans-serif;
	font-size: 9px;
	border: solid 1px #EEE;
	width: 60px;
}

.eXtremeTable .tableRegion {
	border: 0px solid silver;
	padding: 1px;
	font-size: 10px;
	margin-top: 0px; 
	cellspacing: 1px ;
	cellpadding: 0px ;
}

.eXtremeTable .filter {
	background-color: #F4F4F4;
}

.eXtremeTable .filter td {
	padding: 2px;
}

.eXtremeTable .title {
	color: #444444;
	font-weight: bold;
	font-family: verdana, arial, helvetica, sans-serif;
	font-size: 15px;
	vertical-align: middle;
}

.eXtremeTable .titleRow {
	background-color: #F4F4F4;
}

.eXtremeTable .titleRow td {
	border-bottom: 1px solid #308dbb;
	padding-top: 2px;
	padding-bottom: 2px;
	vertical-align: middle;
	text-align: center;
}

.eXtremeTable .titleRow span {
	color: #444444;
	font-weight: bold;
	font-size: 12px;
}

.eXtremeTable .filter input {
	font-family: verdana, arial, helvetica, sans-serif;
	font-size: 12px;
	border: solid 1px grey;
	padding: 1px;
}

.eXtremeTable .filter select {
	font-family: verdana, arial, helvetica, sans-serif;
	font-size: 9px;
	border: solid 1px #EEE;
}

.eXtremeTable INPUT.radio {
	font-size:9px;
	width: auto;
}

.eXtremeTable INPUT.checkbox {
	font-size:9px;
	width: auto;
	border: 0;
	background-color: transparent;
}

.eXtremeTable .tableHeader {
	overflow:hidden;
	white-space:nowrap; 
	height: 26px;
	background-color: #EBEBEB;
	color: #7F7F7F;
	font-family: "ו";
	font-size: 12px;
	font-weight: bold;
	text-align: center;
	padding-right: 3px;
	padding-left: 3px;
	padding-top: 3px;
	padding-bottom: 3px;
	margin: 0px;
	border-width:0px;
	border-color: #97C8E1 #063750 #063750 #97C8E1;

}

.eXtremeTable .tableHeaderSort {
	overflow:hidden;
	white-space:nowrap; 
	height: 20px;
	background-color: #3a95c2;
	color: white;
	font-family: "ו";
	font-size: 12px;
	font-weight: bold;
	text-align: center;
	padding-right: 3px;
	padding-left: 3px;
	padding-top: 3px;
	padding-bottom: 3px;
	margin: 0px;
	border-style: solid;
	border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-color: #97C8E1 #063750 #063750 #97C8E1;

}

.eXtremeTable .odd a, .even a {
	color: Black;
	font-size: 12px;
}

.eXtremeTable .odd td, .eXtremeTable .even td {
	overflow:hidden;
	white-space:nowrap; 
	padding-top: 2px;
	padding-right: 3px;
	padding-bottom: 2px;
	padding-left: 3px;
	vertical-align: middle;
	font-family: verdana, arial, helvetica, sans-serif;
	font-size: 12px;
}

.eXtremeTable td {
	overflow:hidden;
	white-space:nowrap; 
	height: 28px;
	padding-top: 2px;
	padding-right: 3px;
	padding-bottom: 2px;
	padding-left: 3px;
	vertical-align: middle;
	font-size: 12px;
	text-align:center;
	color:#7F7F7F;
}

.eXtremeTable th {
	overflow:hidden;
	white-space:nowrap; 
	height: 20px;
	padding-top: 2px;
	padding-right: 3px;
	padding-bottom: 2px;
	padding-left: 3px;
	vertical-align: middle;
	font-family: verdana, arial, helvetica, sans-serif;
	font-size: 12px;
}
.eXtremeTable tr {
	padding-top: 2px;
	padding-right: 3px;
	padding-bottom: 2px;
	padding-left: 3px;
	vertical-align: middle;
	font-size: 12px;
	background-color: white;
}


.tableRegion thead tr th {
	top:expression(this.offsetParent.scrollTop);
	overflow:hidden;
	white-space:nowrap; 
	height: 20px;
	background-color: #6699CC;
	color: white;
	font-family: "ו";
	font-size: 12px;
	font-weight: bold;
	text-align: center;
	padding-right: 3px;
	padding-left: 3px;
	padding-top: 3px;
	padding-bottom: 3px;
	margin: 0px;
	border-style: solid;
	border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-color: #97C8E1 #063750 #063750 #97C8E1;
}

.eXtremeTable .odd {
	overflow:hidden;
	white-space:nowrap; 
	cursor: hand;
	border-style: solid;
	background-color: #FAFAFA;
}

.eXtremeTable .even {
  	overflow:hidden;
	white-space:nowrap; 
	cursor: hand;
	border-style: solid;
	background-color: #dfe4e8;
}

.eXtremeTable .hand {
	cursor: hand;
  	border-style: solid;
}

.eXtremeTable .highlight td {
	cursor: hand;
	color: black;
	font-size: 12px;
	padding-top: 2px;
	padding-right: 3px;
	padding-bottom: 2px;
	padding-left: 3px;
	vertical-align: middle;
	background-color: #fdecae;
}

.eXtremeTable .highlight a, .highlight a {
	color: black;
	font-size: 12px;
}

.eXtremeTable .compactToolbar {
	background-color: #F4F4F4;
}

.eXtremeTable .toolbar {
	background-color: #F4F4F4;
	font-family: verdana, arial, helvetica, sans-serif;
	font-size: 9px;
	margin-right: 1px;
	border-right: 1px solid silver;
	border-left: 1px solid silver;
	border-top: 1px solid silver;
	border-bottom: 1px solid silver;
}

.eXtremeTable .toolbar a:hover {
	background-color: transparent;
}

.eXtremeTable a,a.udline:hover {
	text-decoration: underline;
}

.eXtremeTable .toolbar td {
	color: #444444;
	padding: 0px 3px 0px 3px;
	text-align:center;
}

.eXtremeTable .separator {
	width: 7px;
}

.eXtremeTable .statusBar {
	background-color: #F4F4F4;
	font-family: verdana, arial, helvetica, sans-serif;
	font-size: 12px;
}

.eXtremeTable .filterButtons {
	background-color: #F4F4F4;
	text-align: right;
}

.eXtremeTable .title span {
	margin-left: 7px;
}

.eXtremeTable .formButtons {
	display: block;
	margin-top: 10px;
	margin-left: 5px;
}

.eXtremeTable .formButton {
	cursor: pointer;
	font-family: verdana, arial, helvetica, sans-serif;
	font-size:10px;
	font-weight: bold;
	background-color: #308dbb;
	color: white;
	margin-top: 5px;
	border: outset 1px #333;
	vertical-align: middle;
}

.eXtremeTable .calcRow {
	background-color: #F0F0F0;
	font-size:12px;
	height:22px	
}

.eXtremeTable .calcTitle {
	background-color: #F0F0F0;
	color: black;
	text-align: center;
	font-family: verdana, arial, helvetica, sans-serif;
	font-weight: bold;
	font-size: 12px;
}

.eXtremeTable .calcResult {
	background-color: #F0F0F0;
	color: black;
	text-align: right;
	font-family: verdana, arial, helvetica, sans-serif;
	font-size:12px;
}

.eXtremeTable .selectedtr {  
	cursor: hand;
	color: black;
	font-size: 12px;
	border-style: solid;
	padding-top: 2px;
	padding-right: 3px;
	padding-bottom: 2px;
	padding-left: 3px;
	vertical-align: middle;
	background-color: #E6E4F7;
}

